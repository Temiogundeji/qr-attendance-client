{
  "expo": {
    "name": "qr-att-client",
    "slug": "qr-att-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/polytendance.png",
    "splash": {
      "image": "./assets/splashy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.temiogundeji.qrattclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This app was built to make it easier for schools to manage their student's attendance with ease."
  }
}
